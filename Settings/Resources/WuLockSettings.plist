			<key>cellClass</key>
			<string>WUSwitchCell</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		<!-- Set custom bio fail text -->
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string></string>
			<key>key</key>
			<string>CustomBioText</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<string>Shame on a finga</string>
			<key>placeholder</key>
			<string>Shame on a finga</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		
		
		
		
		<!-- [] Auto-dim timeout -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Auto-dim timeout</string>
		</dict>
		
		<!-- Timeout -->
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Timeout</string>
			<key>keyboard</key>
			<string>numbers</string>
			<key>key</key>
			<string>DimTimeout</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<string>8</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		
		
		
		
		<!-- [] Support -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Support</string>
		</dict>
		
