		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>No delay</string>
			<key>key</key>
			<string>NoDelay</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<true/>
			<key>cellClass</key>
			<string>WUSwitchCell</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		<!-- Hide chevron -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Hide chevron</string>
			<key>key</key>
			<string>HideChevron</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<true/>
			<key>cellClass</key>
			<string>WUSwitchCell</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		<!-- Use custom text -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Use custom text?</string>
			<key>key</key>
			<string>UseCustomText</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<true/>
			<key>cellClass</key>
			<string>WUSwitchCell</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		<!-- Set custom text -->
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string></string>
			<key>key</key>
			<string>CustomText</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<string>Protect ya neck</string>
			<key>placeholder</key>
			<string>Protect ya neck</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
			<key>alignment</key>
			<integer>1</integer>
		</dict>
		
		
		
		
		
		<!-- [] TouchID failed -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Touch ID failure</string>
		</dict>
		
		<!-- Use custom bio fail text -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Use custom text?</string>
			<key>key</key>
			<string>UseCustomBioText</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
