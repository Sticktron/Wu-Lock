<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>title</key>
	<string>Wu-Lock</string>
	
	<key>items</key>
	<array>
		
		<!-- [] Header -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>headerCellClass</key>
			<string>WULogoCell</string>
		</dict>
		
		<!-- Enabled Switch -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Enabled</string>
			<key>key</key>
			<string>Enabled</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<true/>
			<key>cellClass</key>
			<string>WUSwitchCell</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		
		
		<!-- [] Appearance -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Appearance</string>
		</dict>
		
		<!-- Image chooser -->
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>label</key>
			<string>Choose Image</string>
			<key>detail</key>
			<string>WUImageController</string>
			<key>cellClass</key>
			<string>WULinkCell</string>
		</dict>
		
		<!-- Style list -->
		<dict>
			<key>cell</key>
			<string>PSLinkListCell</string>
			<key>label</key>
			<string>Style</string>
			<key>detail</key>
			<string>PSListItemsController</string>
			
			<key>validTitles</key>
			<array>
				<string>White</string>
				<string>Black</string>
				<string>Yellow</string>
				<string>Vibrant blur</string>
				<string>Burnt blur</string>
				<string>Original</string>
			</array>
			
			<key>validValues</key>
			<array>
				<string>white</string>
				<string>black</string>
				<string>yellow</string>
				<string>vibrantBlur</string>
				<string>burntBlur</string>
				<string>original</string>
			</array>
			
			<key>key</key>
			<string>Style</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<string>vibrantblur</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		
		
		<!-- [] Position -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Adjust Position</string>
			<key>footerText</key>
			<string>Negative numbers move the glyph down.</string>
		</dict>
		
		<!-- Y-offset -->
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Y Offset</string>
			<key>keyboard</key>
			<string>numbers</string>
			<key>key</key>
			<string>YOffset</string>
			<key>defaults</key>
			<string>com.sticktron.wu-lock</string>
			<key>default</key>
			<string>0</string>
			<key>PostNotification</key>
			<string>com.sticktron.wu-lock.settingschanged</string>
		</dict>
		
		
		
		<!-- [] Slide to unlock -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Slide to unlock</string>
		</dict>
		
